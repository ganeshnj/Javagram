<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPaneCrop" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <!-- TODO Add Nodes -->
  <children>
    <BorderPane fx:id="borderPaneCrop" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <top>
        <VBox>
          <children>
            <HBox>
              <children>
                <Button mnemonicParsing="false" onAction="#backAction" text="Back">
                  <HBox.margin>
                    <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" fx:id="x1" />
                  </HBox.margin>
                </Button>
                <Region HBox.hgrow="ALWAYS" />
                <Button mnemonicParsing="false" onAction="#nextAction" text="Next" HBox.margin="$x1"></Button>
              </children>
            </HBox>
          </children>
        </VBox>
      </top>
    </BorderPane>
  </children>
</AnchorPane>
