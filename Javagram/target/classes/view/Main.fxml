<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controller.MainController">
  <!-- TODO Add Nodes -->
  <children>
    <BorderPane fx:id="rootBorderPane" center="$null" prefHeight="455.0" prefWidth="748.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <bottom>
        <HBox>
          <children>
            <Label text="Label">
              <HBox.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
              </HBox.margin>
            </Label>
          </children>
        </HBox>
      </bottom>
      <left>
        <TreeView fx:id="treeViewLibrary" prefHeight="389.0" prefWidth="252.0" />
      </left>
      <top>
        <VBox>
          <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#newFolderAction" text="Add a folder" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#showThumbnailsAction" text="Show thumbnails" />
                    <MenuItem mnemonicParsing="false" onAction="#showPreviewAction" text="Show Preview" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
          </children>
        </VBox>
      </top>
    </BorderPane>
  </children>
</AnchorPane>
